<% if(libs.includes('pixi')) { %><script src="./libs/pixi/pixi.min-v7.js"></script><% } -%>
<% if(libs.includes('pixi')) { %><script src="./libs/pixi/pixi-sound-v7.js"></script><% } -%>
<% if(appType == 'cshtml') { -%>
<script type="text/javascript">
    window.urlPrefix = "/hntc/2024qixi/";
    ~(new function (_configValue) {
        let configValue = null;
        try {
            configValue = JSON.parse(_configValue)
        } catch {
            return;
        }
        let configs = Object.assign({}, configValue);
        stalker.update({ uid: configs.profiles.openId });
        this.get = function (callback) { callback && callback.call(null, configs); };
        window.globalConfig = this;
        window.globalConfig.get();
    }('@(Html.Raw(ViewBag.globalConfigs))'));
    var locHref = h5fx.queryParse(decodeURIComponent(location.href)) || {};
</script><% } -%>
<% if(libs.includes('lucky-draw')) { %><script src="./libs/lucky-draw.js"></script><% } %>
<% if(libs.includes('clipboard')) { %><script src="./libs/clipboard.min.js"></script><% } %>
<% if(libs.includes('timer')) { %><script src="./libs/timer.js"></script><% } %>
<script src="./scripts/index.js"></script>
<% if(libs.includes('stalker')) { -%>
<!--removeIf(production)-->
<script type="text/javascript">
    location.host.indexOf("192.168.1") >= 0 && (stalker = { tail: () => {}, pagein: () => {} })
</script>
<!--endRemoveIf(production)-->
<% } -%>
<script type="text/javascript">
$(function(){
<% if(libs.includes('layer-mobile')) { -%>
    window.popupToast = function (msg, time) {
        layer.open({
            content: msg,
            skin: 'msg',
            time: time || 2
        });
    };
<% } -%>
<% if(libs.includes('wxjssdk')) { -%>
    const jssdkApiHandler = location.host.indexOf("192.168.1") >= 0 
        ? new Promise(resolve => resolve(''))
        : new Promise(resolve => h5fx.http.get("https://????.????.com/wx/ddhd/jssdk?url=" + encodeURIComponent(location.href)).then(res => resolve(res.data)));
            
    h5fx.device.isWechat && jssdkApiHandler.then(function (data) {
        !!data && h5fx.wxJssdk.init({
            debug: false,
            appId: data.appId,
            timestamp: data.timestamp,
            nonceStr: data.nonceStr,
            signature: data.signature,
            ready: function () {
                this.hideH5UnneededMenus();
                this.share({
                    title: 'title',
                    desc: 'desc',
                    timeline: 'timeline',
                    link: location.origin + '/hntc/2024qixi/',
                    imgUrl: location.origin + "/hntc/2024qixi/images/share.png",
                    success: function (type) {
<% if(libs.includes('stalker')) { %>
                        stalker.tail('微信分享', type);
<% } %>
                    }
                });
            }
        });
    });
<% if(libs.includes('stalker')) { -%>
    stalker.pagein();
<% } -%>
});
<% } -%>
</script>