<% if(libs.includes('layer-mobile')) { %><script src="./libs/layer-mobile/layer-mobile.js"></script><% } %>
<% if(libs.includes('html2canvas')) { %><script src="./libs/html2canvas.1.4.1.js"></script><% } -%>
